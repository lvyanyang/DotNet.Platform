@model PageList<DotNet.Auth.Entity.Version>
<table class="table table-striped table-bordered table-hover table-manage">
    <thead>
        <tr>
            <th class="w70px">版本</th>
            <th class="w70px">类型</th>
            <th class="w70px">操作者</th>
            <th class="w160px">操作时间</th>
        </tr>
    </thead>
    <tbody>
        @for (int index = 0; index < Model.Count; index++)
        {
            var record = Model[index];
            <tr>
                <td>@(index + 1)</td>
                <td>@record.CategoryName</td>
                <td>@record.CreateUserName</td>
                <td>@DateTimeHelper.FormatDateHasSecond(record.CreateDateTime)</td>
            </tr>
        }
    </tbody>
</table>
@Html.RenderPages(Model)