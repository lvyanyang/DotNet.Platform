        <div class="container-index header-main">
            <div class="logo-box f-left">
                <img src="/www/img/logo.png" alt="道路运输从业人员继续教育学习平台" width="300" height="70" />
            </div>
        </div>
    </div>

    <div class="usermain">
        <div class="leftmenu link_bk">
            @Html.Partial("_Navbar")
        </div>
        <div class="rightmain">
            @RenderBody()
        </div>
        <div class="clear"></div>
    </div>

    @Html.Partial("_Bottom")
    <div class="back_to_top" id="back_to_top" title="回到顶部">
        <div class="back" style="display: none;">
        </div>
    </div>
    
    @Html.CdnImport("~/lib.js")
    @Html.CdnImport("~/fx.js")
